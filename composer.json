{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "askedio/laravel-soft-cascade": "^8.1",
        "barryvdh/laravel-ide-helper": "^2.6",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.0",
        "irazasyed/telegram-bot-sdk": "^3.0",
        "jeremykenedy/laravel-roles": "^2.1",
        "jhaoda/socialite-mailru": "^3.0",
        "laravel/framework": "^7.0",
        "laravel/socialite": "^4.1",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "meditationsteps/blogetc": "^8.4",
        "owen-it/laravel-auditing": "^11.0",
        "predis/predis": "^1.1",
        "silviolleite/laravelpwa": "^2.0",
        "socialiteproviders/vkontakte": "^4.0",
        "socialiteproviders/yandex": "^3.0",
        "spatie/laravel-medialibrary": "^8.10",
        "staudenmeir/belongs-to-through": "^2.5",
        "staudenmeir/eloquent-has-many-deep": "1.12",
        "symfony/process": "^5.1"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "jn-jairo/laravel-ngrok": "^2.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories",
            "app/Models"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
